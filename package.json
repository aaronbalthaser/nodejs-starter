{
  "name": "node_api_starter",
  "version": "1.0.0",
  "description": "Public / Private API - Print Eezy",
  "main": "api.js",
  "repository": "",
  "author": "Aaron Balthaser",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/api.js",
    "prod": "nodemon dist/src/api.js",
    "prebuild": "del-cli dist/src/*",
    "build": "babel src --out-dir dist/src --copy-files && yarn copy",
    "copy": "cp package.json dist/ && cp yarn.lock dist/ && cp .gitignore dist/ && cp .env dist/ && cp .nvmrc dist/",
    "tdd": "mocha --watch \"./tests/**/*.js\" --recursive"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "del-cli": "^1.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "helmet": "^3.16.0",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.16.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.9.0",
    "rbac2": "^1.1.0",
    "recursive-readdir-sync": "^1.0.6",
    "sequelize": "^5.8.1",
    "underscore": "^1.9.1"
  }
}